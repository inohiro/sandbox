require 'rdf'
require 'pp'

FILE_PATH = '/home/inohiro/Downloads/unknown_rdfs/file001.nt'

def main

=begin
  RDF::NTriples::Reader.open( FILE_PATH ) do |reader|
    reader.each do |stm|
      pp stm
      gets
    end
  end
=end

=begin
  graph = RDF::Graph.load( FILE_PATH ) do
#  RDF::Graph.load( FILE_PATH ) do |graph|
    graph.each do |stm|
      pp stm
      gets
    end
  end
=end

  RDF::Reader.for( :ntriples ).new( FILE_PATH ) do |reader|
    reader.each do |stm|
      pp stm
      gets
    end
  end

end

main
